<div class="container">
  <h1><%= @key_word.name %></h1>
  <%= link_to 'Edit keyword', edit_key_word_path %>
  <%= link_to 'Delete keyword', key_word_path(@key_word), method: :delete %>
  <%= link_to 'All keywords', key_words_path %>

  <h1>Tweets</h1>
  <% @tweets.each do |tweet| %>
    <p><%= tweet.text %>(pubished <%= time_ago_in_words tweet.created_at %> ago)</p>
  <% end %>
</div>
